/**
 * Implements hook_islandora_oai_get_xsl_files().
 */
function wwudora_islandora_oai_get_xsl_files() {
  $module_path = drupal_get_path('module', 'wwudora');
  return array(
    "$module_path/xml/wwu_mods_to_dc.xsl" => 'wwu_mods_to_dc.xsl',
  );
}